{
    "name": "mostlyserious/calendarize",
    "description": "A calendar field type providing functionality for recurrence.",
    "type": "craft-plugin",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "calendarize"
    ],
    "support": {
        "docs": "https://github.com/mostlyserious/calendarize",
        "issues": "https://github.com/mostlyserious/calendarize/issues?state=open"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Mostly Serious",
            "homepage": "https://mostlyserious.io"
        },
        {
            "name": "Franco Valdes",
            "homepage": "https://union.co"
        }
    ],
    "require": {
        "craftcms/cms": "^4.0|^5.0",
        "rlanvin/php-rrule": "~1.6.0"
    },
    "autoload": {
        "psr-4": {
          "mostlyserious\\calendarize\\": "src/"
        }
    },
    "extra": {
        "name": "Calendarize",
        "handle": "calendarize",
        "documentationUrl": "https://github.com/mostlyserious/calendarize/blob/master/README.md",
        "changelogUrl": "https://raw.githubusercontent.com/mostlyserious/calendarize/master/CHANGELOG.md",
        "class": "mostlyserious\\calendarize\\Calendarize"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "craftcms/rector": "dev-main"
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    }
}
